<h1>Listing medias</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Alias</th>
    <th>Menu label</th>
    <th>File</th>
    <th>Media category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @medias.each do |media| %>
  <tr>
    <td><%= media.title %></td>
    <td><%= media.alias %></td>
    <td><%= media.menu_label %></td>
    <td><%= media.file %></td>
    <td><%= media.media_category_id %></td>
    <td><%= link_to 'Show', media %></td>
    <td><%= link_to 'Edit', edit_media_path(media) %></td>
    <td><%= link_to 'Destroy', media, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Media', new_media_path %>
